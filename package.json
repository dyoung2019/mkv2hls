{
  "name": "mkv2hls",
  "version": "1.0.1",
  "exports": {
    ".": "./dist/index.js",
    "./es6": "./lib/index.js"
  },
  "typesVersions": {
    "*": {
      ".": [
        "./types/index.d.ts"
      ],
      "es6": [
        "./types/index.d.ts"
      ]
    }
  },
  "description": "converts file1.mkv into hls video parts",
  "files": [
    "dist/*",
    "lib/**",
    "types/*"
  ],
  "scripts": {
    "build": "npm run build:es6 && npm run build:cjs",
    "build:es6": "tsc -p tsconfig-mjs.json && node ./deploy/copy-mjs.js",
    "build:cjs": "tsc -p tsconfig-cjs.json && webpack",
    "test": "npm run test:build && npm run test:es6",
    "test:build": "tsc -p tsconfig-test.json",
    "test:es6": "node --experimental-vm-modules --experimental-json-modules node_modules/.bin/jest",
    "dev": "serve demo",
    "clean": "npm run clean:es6 && npm run clean:lib",
    "clean:lib": "node ./deploy/delete-lib.js",
    "clean:es6": "tsc --build --clean tsconfig-mjs.json",
    "clean:cjs": "tsc --build --clean tsconfig-cjs.json"
  },
  "keywords": [
    "mkv",
    "hls",
    "ffmpeg"
  ],
  "author": "David Young",
  "license": "ISC",
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/jest": "^26.0.24",
    "@types/node": "^14.14.37",
    "axios": "^0.21.0",
    "del": "^6.0.0",
    "glob": "^7.1.7",
    "jest": "^27.0.6",
    "memfs": "^3.2.0",
    "prettier": "^2.2.1",
    "serve": "^11.3.2",
    "ts-jest": "^27.0.3",
    "ts-loader": "^8.3.0",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.2",
    "uglify-js": "^3.13.4",
    "uglifyjs-folder": "^2.0.0",
    "webpack": "^5.38.1",
    "webpack-cli": "^4.7.0"
  },
  "dependencies": {
    "ffmpeg-cli": "^2.7.1",
    "yargs": "^16.1.0"
  }
}
